func (s *Block) Do(ctx context.Context, headers ...*common.Header) (result types.Block, err error) {
  var response models.BlockResponse

  s.p.Format = "msgpack"
  err = s.c.getMsgpack(ctx, &response, fmt.Sprintf("/v2/blocks/%d", s.round), s.p, headers)
